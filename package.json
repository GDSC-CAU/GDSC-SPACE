        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "husky": "^8.0.3",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "lint-staged": "^13.2.3",
        "postcss": "8.4.24",
        "prettier": "2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "tailwindcss": "3.3.2",
        "tailwindcss-3d": "^0.3.0",
